	        <parameter type="GtkWidget*" name="widget"/>
		<parameter type="GtkStateType" name="state"/>
		<parameter type="ClutterColor*" name="color" pass_as="out"/>
	      </parameters>
	    </method>
	    <method name="TextureSetFromPixbuf" cname="gtk_clutter_texture_set_from_pixbuf" shared="true">
	      <return-type type="void"/>
	      <parameters>
	        <parameter type="ClutterTexture*" name="texture"/>
		<parameter type="GdkPixbuf*" name="pixbuf"/>
	      </parameters>
	    </method>
	    <method name="TextureSetFromStock" cname="gtk_clutter_texture_set_from_stock" shared="true">
	      <return-type type="void"/>
	      <parameters>
	        <parameter type="ClutterTexture*" name="texture"/>
		<parameter type="GtkWidget*" name="widget"/>
		<parameter type="const-gchar*" name="stock_id"/>
		<parameter type="GtkIconsize" name="size"/>
	      </parameters>
	    </method>
	    <method name="TextureSetFromIconName" cname="gtk_clutter_texture_set_from_icon_name" shared="true">
	      <return-type type="void"/>
	      <parameters>
	        <parameter type="ClutterTexture*" name="texture"/>
		<parameter type="GtkWidget*" name="widget"/>
		<parameter type="const-gchar*" name="icon_name"/>
		<parameter type="GtkIconsize" name="size"/>
	      </parameters>
	    </method>
	  </struct>
	</add-node>
</metadata>

